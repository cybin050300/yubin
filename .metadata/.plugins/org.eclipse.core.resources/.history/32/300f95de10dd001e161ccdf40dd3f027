package com.cybmodifier;

// 접근제한자 테스트용 클래스
public class TestClass {

	// 변수에 적용
	// 1. default
	int defaultVar = 0;

	// 2. private
	private int privateVar = 10;

	// 3. public
	public int publicVar = 100;

	
	public TextClass() {
			
	void defaultM() {
	}

	private void privateM() {
	}

	public void publicM() {
		
	}
	}

}
