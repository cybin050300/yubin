package com.cyb.split;

// 연산전용, 중복되는 코드틀, 라이브러리
public class MethodClass {

	public MethodClass() {
		
		
	}
	// 나이합 메서드
		public void sum(MemberClass[] mc) {
				int sum = 0;
				for(int i=0; i<mc.length;i++) {
					sum=sum+mc[i].age;
				}
				System.out.println(sum);
			}
		// 평균 메서드
		public void avg() {
			
		}
		// 이름 검색 메서드
		public void searchName() {
			
		}
		public void sum() {
			
			
		}

}
