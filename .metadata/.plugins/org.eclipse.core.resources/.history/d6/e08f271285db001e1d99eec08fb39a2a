package com.cyb.employee;

public class EMPMainClass {	
	
	public static void main(String[] args) {
		//EMPDateClass 생성
		EMPDataClass emp1 = new EMPDataClass();	 // 타입을 EMPDataClass() 타입 그대로 가져와라( int, String, float....등등...)
		EMPDataClass emp2 = new EMPDataClass(); 
		
		emp1.age=200; 	//자바의 모든 조상은 object 클래스!!
		emp2.age=400;
		emp1.test(3);
		
		System.out.println(emp1.age + "\t" + emp2.age);
	}
}
