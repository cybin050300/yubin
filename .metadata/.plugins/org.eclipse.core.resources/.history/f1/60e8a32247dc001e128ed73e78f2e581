package com.cyb.cafe;

import java.util.ArrayList;

public class MethodClass {

	public MethodClass() {
		
		
		// 상품명 검색 메서드(상품횟수를 반환)
		int searchItem(String Item, CafeClass[] mcArray) {
			int sell=0;
			for(int i=0; i<mcArray.length; i++) {
				if(Item.equals(mcArray[i].Item)) {
					sell =mcArray[i].Item.sell;
				}
			}
			return sell;
			
		} // searchName() END

}
}